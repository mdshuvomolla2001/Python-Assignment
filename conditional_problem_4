weight = float(input("Enter the weight in kilograms: "))
height = float(input("Enter the height in metre: "))

BMI = weight/(height**2)

if BMI < 18.5:
    print("Underweight")
elif 18.5 <= BMI <= 24.9:
    print("Normal")
elif 25 <= BMI <= 29.9:
    print("Overwight")
else:
    print("Obese")