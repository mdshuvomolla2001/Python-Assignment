x = int(input("Enter the coordinates value of x: "))
y = int(input("Enter the coordinates value of y: "))

if x>0 and y>0:
    print("First Quadrant")
elif x<0 and y>0:
    print("Second Quadrant")
elif x<0 and y<0:
    print("Third Quadrant")
elif x>0 and y<0:
    print("Fourth Quadrant")
else:
    print("Erorr!Please, try again")