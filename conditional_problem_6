a= float(input("Enter the first number: "))
operator = input("sum for '+',subtraction for '-', multiplication for '*' and division for '/': ")
b= float(input("Enter the second number: "))
if operator == "+":
    print(a+b)
elif operator == "-":
    print(a-b)
elif operator == "*":
    print(a*b)

elif operator == "/":
    if b == 0:
        print("the resut is not defined")
    else:
        print(a/b)
else:
    print("Error")