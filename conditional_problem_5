temperature = float(input("Enter the temperatur in celcius: "))
F = ((9*temperature)/5)+32
K = temperature + 273.15

if temperature >0:
    print("The temperature celsius to Fahrenheit is", F,"F")
elif temperature <0:
    print("The temperature celsius to Kelvin is", K,"K")
else:
    print("The temperature of celsius is", temperature,"degree C")